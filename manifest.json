{
  "name": "Khan Academy Dots",
  "description": "Translate math notation in Khan Academy Crowdin strings.",
  "author": "Szymon Bubak & Daniel Hollas & Krzysztof Krystek & Robert Pala",
  "version": "3.0.0",
  "permissions": [
    "activeTab",
    "storage"
  ],
   "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "content_scripts": [
    {
      "matches": [
        "http://crowdin.com/translate/khanacademy/*",
        "https://crowdin.com/translate/khanacademy/*"
      ],
      "js": [
        "3rdPartyLibs/zepto.min.js",
        "3rdPartyLibs/keymaster.js",
        "3rdPartyLibs/jipt-hack.js",
        "3rdPartyLibs/translation-assistant.js",
        "app.js"
      ],
      "run_at": "document_idle",
      "all_frames": true
    }
  ],
  "icons": {
    "16": "khan-logo-new-16.png",
    "48": "khan-logo-new-48.png",
    "128": "khan-logo-new-128.png"
  },
  "manifest_version": 2
}
